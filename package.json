{
  "name": "aitomics-ui",
  "version": "1.0.0",
  "description": "A local application for managing Aitomics projects with a block-based code editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently -k \"cd src/renderer && npm start\" \"wait-on http://localhost:3000 && npm run start\"",
    "build": "cd src/renderer && npm run build",
    "build-preload": "tsc src/renderer/src/preload.ts --outDir src/renderer/src --module commonjs",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "aitomics": "^0.1.56",
    "electron": "^28.1.0",
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron-builder": "^24.9.1",
    "jest": "^29.7.0",
    "typescript": "^5.8.3",
    "wait-on": "^7.2.0"
  }
}
